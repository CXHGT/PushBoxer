<UserControl x:Class="PushBoxer.GameScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PushBoxer"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="1620">
    <Grid x:Name="windowGrid" Width="1620" Height="1000">
        <Grid Name="GameGrid" Width="1620" Height="1000" HorizontalAlignment="Left" VerticalAlignment="Top">
            <local:GameImage x:Name="MapImage1" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Grid Name="EntityGrid" Width="1620" Height="1000" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        </Grid>
    </Grid>
</UserControl>
