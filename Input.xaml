<Window x:Class="PushBoxer.Input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PushBoxer"
        mc:Ignorable="d"
        Title="请输入基本信息" Height="240" Width="700">
    <Grid  Height="240" Width="700">
        <Label Content="主播身份码" HorizontalAlignment="Left" Margin="11,66,0,0" VerticalAlignment="Top" FontSize="20"/>

        <TextBox Name="upCode" Margin="132,70,0,0" Height="35" VerticalAlignment="Top" HorizontalAlignment="Left" Width="450" FontSize="24" Text=""/>

        <Button Content="确定" HorizontalAlignment="Left" Margin="323,123,0,0" VerticalAlignment="Top" Click="Button_Click" Width="125" Height="45" FontSize="24"/>
        <Button Content="取消" Margin="459,123,0,0" Click="Button_Click_1" HorizontalAlignment="Left" Width="125" Height="45" VerticalAlignment="Top"  FontSize="24"/>
        <Label Content="获取" HorizontalAlignment="Left" Margin="598,68,0,0" VerticalAlignment="Top" MouseDown="Label_MouseDown" Foreground="#FFFF0046" FontSize="20"/>
    </Grid>
</Window>
